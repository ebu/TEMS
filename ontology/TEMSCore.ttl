@prefix tc: <http://tems.org/2024/temscore#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix time: <http://www.w3.org/2006/time#> .
    

<http://tems.org/2024/temscore#> rdf:type owl:Ontology ;
    rdfs:label "TEMSCore Ontology"@en ;
    rdfs:comment "An ontology for the Trusted European Media Data Space"@en ;
    owl:versionInfo "0.1"@en ;
    owl:creator "TEMS T2.3"@en .


# ---------------------------------------------------------------------------
# SKOS Role Definitions (For Contributors) : TO BE COMPLETED
# ---------------------------------------------------------------------------

# Define a SKOS Concept Scheme for Roles related to Contributors
tc:ContributorRoleScheme a skos:ConceptScheme ;
    rdfs:label "Contributor Role Scheme" ;
    skos:prefLabel "Roles in Media Content Creation" ;
    skos:definition "A scheme for defining roles in the creation of media content." .

# Define Core Roles for Contributors as SKOS Concepts
tc:Director a skos:Concept ;
    skos:inScheme tc:ContributorRoleScheme ;
    skos:prefLabel "Director"@en ;
    skos:definition "The role of directing a media production."@en .

tc:Writer a skos:Concept ;
    skos:inScheme tc:ContributorRoleScheme ;
    skos:prefLabel "Writer"@en ;
    skos:definition "The role of writing scripts, articles, or other content."@en .

tc:Producer a skos:Concept ;
    skos:inScheme tc:ContributorRoleScheme ;
    skos:prefLabel "Producer"@en ;
    skos:definition "The role of overseeing the production of media content."@en .

tc:Screenwriter a skos:Concept ;
    skos:inScheme tc:ContributorRoleScheme ;
    skos:prefLabel "Screenwriter"@en ;
    skos:definition "The role of writing the screenplay for a film or television show."@en ;
    skos:broader tc:Writer .


# ---------------------------------------------------------------------------
# SKOS Concept Scheme for ProductionJob Roles
# ---------------------------------------------------------------------------

# Define a SKOS Concept Scheme for Roles related to ProductionJob
tc:ProductionJobRoleScheme a skos:ConceptScheme ;
    rdfs:label "Production Job Role Scheme" ;
    skos:prefLabel "Roles in Media Production" ;
    skos:definition "A scheme for defining roles in the production of media content." .

# Define Core Roles for ProductionJob as SKOS Concepts
tc:Producer a skos:Concept ;
    skos:inScheme tc:ProductionJobRoleScheme ;
    skos:prefLabel "Producer"@en ;
    skos:definition "The role of overseeing the production of media content."@en .

tc:Editor a skos:Concept ;
    skos:inScheme tc:ProductionJobRoleScheme ;
    skos:prefLabel "Editor"@en ;
    skos:definition "The role of editing media content for final production."@en .

tc:CameraOperator a skos:Concept ;
    skos:inScheme tc:ProductionJobRoleScheme ;
    skos:prefLabel "Camera Operator"@en ;
    skos:definition "The role of operating cameras during media production."@en .



# ---------------------------------------------------------------------------
# Class Definitions
# ---------------------------------------------------------------------------

# Define the base class MediaContent
tc:MediaContent a owl:Class ;
    rdfs:label "Media Content" ;
    rdfs:comment "Represents any type of media content, such as articles, videos, podcasts." .

# Define the MediaInstance class
tc:MediaInstance a owl:Class ;
    rdfs:label "Media Instance" ;
    rdfs:comment "Represents a specific version or physical representation of a media content, such as a file with certain properties or formats." .

# Define the OrderedSegment class
tc:OrderedSegment a owl:Class ;
    rdfs:label "Ordered Segment" ;
    rdfs:comment "Represents a segment of media content that is part of an ordered collection." .

# Subclass: Article
tc:Article a owl:Class ;
    rdfs:subClassOf tc:MediaContent;
    rdfs:label "Article" ;
    rdfs:comment "Represents a written article or text-based content. An article may contain other media segments." .

# Subclass: Video
tc:Video a owl:Class ;
    rdfs:subClassOf tc:MediaContent;
    rdfs:label "Video" ;
    rdfs:comment "Represents video content, such as movies, clips, or documentaries. A video can be a standalone media content or part of another media content." .

# Subclass: Image
tc:Image a owl:Class ;
    rdfs:subClassOf tc:MediaContent;
    rdfs:label "Image" ;
    rdfs:comment "Represents an image that can be a standalone media content or part of another media content." .

# Subclass: Audio
tc:Audio a owl:Class ;
    rdfs:subClassOf tc:MediaContent;
    rdfs:label "Audio" ;
    rdfs:comment "Represents audio content, such as podcasts, music, or radio broadcasts. Audio can be a standalone media content or part of another media content." .

# Subclass: Dataset
tc:Dataset a owl:Class ;
    rdfs:subClassOf tc:MediaContent;
    rdfs:label "Dataset" ;
    rdfs:comment "Represents structured data or datasets, such as CSV files, databases, or statistical data. A dataset can be part of another media content." .

# Subclass: 3DObject
tc:3DObject a owl:Class ;
    rdfs:subClassOf tc:MediaContent;
    rdfs:label "3D Object" ;
    rdfs:comment "Represents 3D models or objects, such as CAD files, VR content, or digital sculptures. A 3D object can be part of another media content." .

tc:Event a owl:Class ;
    rdfs:label "Event" ;
    rdfs:comment "Represents a significant occurrence or happening related to media content, publications." .

tc:ContentDescription a owl:Class ;
    rdfs:label "Content Description" ;
    rdfs:comment "Handles the overall description of content, including intrinsic properties and agent-dependent descriptions." .

tc:Descriptor a owl:Class ;
    rdfs:label "Descriptor" ;
    rdfs:comment "Represents a specific descriptive activity performed by an agent, such as generating embeddings or writing a summary." .

tc:Chapter a owl:Class ;
    rdfs:label "Chapter" ;
    rdfs:comment "Represents a structured linear segment of the content, with start and stop points." .

tc:ContentGrouping a owl:Class ;
    rdfs:label "Content Grouping" ;
    rdfs:comment "Represents a group or category of media content, such as a series or collection." .

tc:Concept a owl:Class ;
    rdfs:label "Concept" ;
    rdfs:comment "A tag or category associated with the content." .

# Agent Classes
tc:Agent a owl:Class ;
    rdfs:label "Agent" ;
    rdfs:comment "An entity that performs tasks, such as a person, organization, or digital tool." .

tc:Person a owl:Class ;
    rdfs:subClassOf tc:Agent ;
    rdfs:label "Person" ;
    rdfs:comment "A human individual acting as an agent." .

tc:Organization a owl:Class ;
    rdfs:subClassOf tc:Agent ;
    rdfs:label "Organization" ;
    rdfs:comment "An organized group of people acting as an agent." .

tc:DigitalTool a owl:Class ;
    rdfs:subClassOf tc:Agent ;
    rdfs:label "Digital Tool" ;
    rdfs:comment "A software or digital system acting as an agent." .

# Identifier Class
tc:Identifier a owl:Class ;
    rdfs:label "Identifier" ;
    rdfs:comment "A class representing an identifier along with its provider." .

# Publication Definitions
tc:Publication a owl:Class ;
    rdfs:label "Publication" ;
    rdfs:comment "Represents the publication details of a media content, including access type, monetization model, price, and other related properties." .

# Class for Target Audience
tc:TargetAudience a owl:Class ;
    rdfs:label "Target Audience" ;
    rdfs:comment "Represents the target audience for the content, including socio-demographic data and segments." .

# Provenance Class
tc:Provenance a owl:Class ;
    rdfs:label "Provenance" ;
    rdfs:comment "Represents the history, origin, and modification details of the media content." .

# Rights Class
tc:Rights a owl:Class ;
    rdfs:label "Rights" ;
    rdfs:comment "Covers legal and usage rights associated with the media content." .

# Define the Roles Class (For Contributors)
tc:Role a owl:Class ;
    rdfs:label "Role" ;
    rdfs:comment "Represents the roles that an agent can perform specifically in the creation of media content." .

# Define the Contributors Class
tc:Contributors a owl:Class ;
    rdfs:label "Contributors" ;
    rdfs:comment "Represents the collection of agents involved in the creation of media content." .

# Define the Consumption Class
tc:Consumption a owl:Class ;
    rdfs:label "Consumption" ;
    rdfs:comment "Describes the consumption of media content, focusing on aggregate metrics and audience data." .

# Base TechnicalMetadata class
tc:TechnicalMetadata a owl:Class ;
    rdfs:label "Technical Metadata" ;
    rdfs:comment "Represents the technical properties of media content." .

# Subclass: VideoTechnicalMetadata
tc:VideoTechnicalMetadata a owl:Class ;
    rdfs:subClassOf tc:TechnicalMetadata ;
    rdfs:label "Video Technical Metadata" ;
    rdfs:comment "Represents the technical properties specific to video content." .

# Subclass: AudioTechnicalMetadata
tc:AudioTechnicalMetadata a owl:Class ;
    rdfs:subClassOf tc:TechnicalMetadata ;
    rdfs:label "Audio Technical Metadata" ;
    rdfs:comment "Represents the technical properties specific to audio content." .

# Subclass: ImageTechnicalMetadata
tc:ImageTechnicalMetadata a owl:Class ;
    rdfs:subClassOf tc:TechnicalMetadata ;
    rdfs:label "Image Technical Metadata" ;
    rdfs:comment "Represents the technical properties specific to image content." .

# Subclass: 3DObjectTechnicalMetadata
tc:3DModelTechnicalMetadata a owl:Class ;
    rdfs:subClassOf tc:TechnicalMetadata ;
    rdfs:label "3D Object Technical Metadata" ;
    rdfs:comment "Represents the technical properties specific to 3D model content." .

# Location Class
tc:Location a owl:Class ;
    rdfs:label "Location" ;
    rdfs:comment "Represents the geographic location associated with media content, agents, or events." .

# Define the MediaEvaluation class
tc:MediaEvaluation a owl:Class ;
    rdfs:label "Media Evaluation" ;
    rdfs:comment "Represents the evaluation of media content, specifically for fact-checking and assessing misinformation." .

tc:ProductionJob a owl:Class ;
    rdfs:label "Production Job" ;
    rdfs:comment "Represents a job or task performed to generate media content or its instances." .


# ---------------------------------------------------------------------------
# Object Properties
# ---------------------------------------------------------------------------

# Object Property: Link MediaInstance to MediaContent
tc:isInstanceOf a owl:ObjectProperty ;
    rdfs:label "is instance of" ;
    rdfs:comment "Links a MediaInstance to the MediaContent it represents." ;
    rdfs:domain tc:MediaInstance ;
    rdfs:range tc:MediaContent .

# Inverse Property: Links MediaContent to MediaInstance
tc:hasInstance a owl:ObjectProperty ;
    rdfs:label "has instance" ;
    rdfs:comment "Indicates that a MediaContent has a specific MediaInstance." ;
    rdfs:domain tc:MediaContent ;
    rdfs:range tc:MediaInstance .

# Declare the inverse relationship
tc:isInstanceOf owl:inverseOf tc:hasInstance .

# Object Property: hasOrderedSegment linking MediaContent to OrderedSegment
tc:hasOrderedSegment a owl:ObjectProperty ;
    rdfs:label "has ordered segment" ;
    rdfs:comment "Links MediaContent to its ordered segments." ;
    rdfs:domain tc:MediaContent ;
    rdfs:range tc:OrderedSegment .

# Object Property: hasSegment linking OrderedSegment to MediaContent
tc:hasSegment a owl:ObjectProperty ;
    rdfs:label "has segment" ;
    rdfs:comment "Links an OrderedSegment to a specific media content segment (e.g., video, audio, etc.)." ;
    rdfs:domain tc:OrderedSegment ;
    rdfs:range tc:MediaContent .

# Object Property: isSegmentOf as inverse of hasSegment
tc:isSegmentOf a owl:ObjectProperty ;
    rdfs:label "is segment of" ;
    rdfs:comment "Indicates that a MediaContent is part of an OrderedSegment." ;
    rdfs:domain tc:MediaContent ;
    rdfs:range tc:OrderedSegment .

# Declare the inverse relationship between hasSegment and isSegmentOf
tc:hasSegment owl:inverseOf tc:isSegmentOf .

# Property linking MediaContent to ContentDescription
tc:hasDescription a owl:ObjectProperty ;
    rdfs:label "has description" ;
    rdfs:comment "Links media content to its content description." ;
    rdfs:domain tc:MediaContent ;
    rdfs:range tc:ContentDescription .

# Inverse Property linking ContentDescription to MediaContent
tc:isDescriptionOf a owl:ObjectProperty ;
    rdfs:label "is description of" ;
    rdfs:comment "Indicates that a content description is associated with the media content it describes." ;
    rdfs:domain tc:ContentDescription ;
    rdfs:range tc:MediaContent .

# Declare the inverse relationship
tc:hasDescription owl:inverseOf tc:isDescriptionOf .

# Object Property: hasEvent linking entities to Event
tc:hasEvent a owl:ObjectProperty ;
    rdfs:label "has event" ;
    rdfs:comment "Links an entity, such as media content or publication, to an event." ;
    rdfs:domain owl:Thing ;  # Applied to any class, can be refined for specific classes
    rdfs:range tc:Event .

# Inverse Object Property: isEventFor linking Event to entities
tc:isEventFor a owl:ObjectProperty ;
    rdfs:label "is event for" ;
    rdfs:comment "Indicates that an event is associated with a specific entity, such as media content or publication." ;
    rdfs:domain tc:Event ;
    rdfs:range owl:Thing .

# Declare the inverse relationship between hasEvent and isEventFor
tc:hasEvent owl:inverseOf tc:isEventFor .

# MentionsPerson (linking MediaContent to a Person)
tc:mentionsPerson a owl:ObjectProperty ;
    rdfs:label "Mentions Person" ;
    rdfs:comment "Links media content to a person mentioned in the content." ;
    rdfs:domain tc:MediaContent ;
    rdfs:range tc:Person .

# Property linking ContentDescription to Descriptor
tc:hasDescriptor a owl:ObjectProperty ;
    rdfs:label "has descriptor" ;
    rdfs:comment "Links a content description to one or more descriptors for agent-dependent fields." ;
    rdfs:domain tc:ContentDescription ;
    rdfs:range tc:Descriptor .

# Inverse Property linking Descriptor to ContentDescription
tc:isDescriptorOf a owl:ObjectProperty ;
    rdfs:label "is descriptor of" ;
    rdfs:comment "Indicates that a descriptor is part of or belongs to a specific content description." ;
    rdfs:domain tc:Descriptor ;
    rdfs:range tc:ContentDescription .

# Declare the inverse relationship
tc:hasDescriptor owl:inverseOf tc:isDescriptorOf .

tc:hasChapter a owl:ObjectProperty ;
    rdfs:label "has chapter" ;
    rdfs:comment "Links a content description or descriptor to one or more chapters." ;
    rdfs:domain tc:ContentDescription ;
    rdfs:range tc:Chapter .

# Property linking Descriptor to Agent
tc:isDescribedBy a owl:ObjectProperty ;
    rdfs:label "is described by" ;
    rdfs:comment "Links a descriptor to the agent who performed the description." ;
    rdfs:domain tc:Descriptor ;
    rdfs:range tc:Agent .

# Inverse Property linking Agent to Descriptor
tc:performsDescription a owl:ObjectProperty ;
    rdfs:label "performs description" ;
    rdfs:comment "Indicates that an agent performs the creation of the descriptor." ;
    rdfs:domain tc:Agent ;
    rdfs:range tc:Descriptor .

# Declare the inverse relationship
tc:isDescribedBy owl:inverseOf tc:performsDescription .


# Object Property linking MediaContent to ContentGrouping
tc:hasGrouping a owl:ObjectProperty ;
    rdfs:label "Has Grouping" ;
    rdfs:comment "Associates media content with a content grouping that categorizes or organizes it." ;
    rdfs:domain tc:MediaContent ;
    rdfs:range tc:ContentGrouping .

# Inverse Property linking ContentGrouping to MediaContent
tc:isGroupingOf a owl:ObjectProperty ;
    rdfs:label "Is Grouping Of" ;
    rdfs:comment "Indicates that the content grouping categorizes or organizes specific media content." ;
    rdfs:domain tc:ContentGrouping ;
    rdfs:range tc:MediaContent .

# Declare the inverse relationship
tc:hasGrouping owl:inverseOf tc:isGroupingOf .

# Relationships Involving Concepts
tc:hasConcept a owl:ObjectProperty ;
    rdfs:label "has concept" ;
    rdfs:comment "Associates the content with tags or categories." ;
    rdfs:domain tc:ContentDescription ;
    rdfs:range tc:Concept .

# Relationships Involving Agents

# Object Property: Links an entity to its identifier
tc:hasIdentifier a owl:ObjectProperty ;
    rdfs:label "has identifier" ;
    rdfs:comment "Links an entity to its identifier." ;
    rdfs:range tc:Identifier .

# Inverse Property: Links an identifier to the entity it identifies
tc:isIdentifierOf a owl:ObjectProperty ;
    rdfs:label "is identifier of" ;
    rdfs:comment "Indicates that an identifier is associated with a specific entity." ;
    rdfs:domain tc:Identifier ;
    rdfs:range owl:Thing .

# Declare the inverse relationship between hasIdentifier and isIdentifierOf
tc:hasIdentifier owl:inverseOf tc:isIdentifierOf .

# Object Property linking Publication to Agent (Publisher)
tc:hasPublisher a owl:ObjectProperty ;
    rdfs:label "Has Publisher" ;
    rdfs:comment "Links a publication to its publisher." ;
    rdfs:domain tc:Publication ;
    rdfs:range tc:Agent .

# Inverse Property linking Agent to Publication
tc:isPublisherOf a owl:ObjectProperty ;
    rdfs:label "Is Publisher Of" ;
    rdfs:comment "Indicates that the agent is the publisher of a specific publication." ;
    rdfs:domain tc:Agent ;
    rdfs:range tc:Publication .

# Declare the inverse relationship
tc:hasPublisher owl:inverseOf tc:isPublisherOf .

# Object Property linking MediaContent to Publication
tc:hasPublication a owl:ObjectProperty ;
    rdfs:label "Has Publication" ;
    rdfs:comment "Links media content to its publication details." ;
    rdfs:domain tc:MediaContent ;
    rdfs:range tc:Publication .

# Inverse Property linking Publication to MediaContent
tc:isPublicationOf a owl:ObjectProperty ;
    rdfs:label "Is Publication Of" ;
    rdfs:comment "Indicates that the publication is associated with specific media content." ;
    rdfs:domain tc:Publication ;
    rdfs:range tc:MediaContent .

# Declare the inverse relationship
# Declare the inverse relationship
tc:isPublicationOf owl:inverseOf tc:hasPublication .

# Object Property linking Publication to TargetAudience
tc:hasTargetAudience a owl:ObjectProperty ;
    rdfs:label "Has Target Audience" ;
    rdfs:comment "Links the publication to its target audience." ;
    rdfs:domain tc:MediaContent ;
    rdfs:range tc:TargetAudience .

# Inverse Property linking TargetAudience to Publication
tc:isTargetAudienceFor a owl:ObjectProperty ;
    rdfs:label "Is Target Audience For" ;
    rdfs:comment "Indicates that the target audience is the intended audience for specific publications." ;
    rdfs:domain tc:TargetAudience ;
    rdfs:range tc:MediaContent .

# Declare the inverse relationship
tc:hasTargetAudience owl:inverseOf tc:isTargetAudienceFor .

# Object Property linking MediaContent to Rights
tc:hasRights a owl:ObjectProperty ;
    rdfs:label "Has Rights" ;
    rdfs:comment "Links media content to its rights information." ;
    rdfs:domain tc:MediaContent ;
    rdfs:range tc:Rights .

# Inverse Property linking Rights to MediaContent
tc:isRightsOf a owl:ObjectProperty ;
    rdfs:label "Is Rights Of" ;
    rdfs:comment "Indicates that the rights are associated with specific media content." ;
    rdfs:domain tc:Rights ;
    rdfs:range tc:MediaContent .

# Declare the inverse relationship
tc:hasRights owl:inverseOf tc:isRightsOf .

# Object Property linking Rights to Agent
tc:isHeldBy a owl:ObjectProperty ;
    rdfs:label "Is Held By" ;
    rdfs:comment "Indicates that the rights are held by a specific agent." ;
    rdfs:domain tc:Rights ;
    rdfs:range tc:Agent .

# Inverse Property linking Agent to Rights
tc:holdsRightsFor a owl:ObjectProperty ;
    rdfs:label "Holds Rights For" ;
    rdfs:comment "Indicates that an agent holds the rights for specific content." ;
    rdfs:domain tc:Agent ;
    rdfs:range tc:Rights .

# Declare the inverse relationship
tc:isHeldBy owl:inverseOf tc:holdsRightsFor .

# Generic Object Property: hasContributor
tc:hasContributor a owl:ObjectProperty ;
    rdfs:label "has contributor" ;
    rdfs:comment "Links an entity to its contributors." ;
    rdfs:domain owl:Thing ;
    rdfs:range tc:Agent .

# Inverse Property for hasContributor (applies automatically to subproperties)
tc:isContributorOf a owl:ObjectProperty ;
    rdfs:label "is contributor of" ;
    rdfs:comment "Indicates that an agent is a contributor of an entity." ;
    rdfs:domain tc:Agent ;
    rdfs:range owl:Thing .

# Declaring the inverse relationship
tc:hasContributor owl:inverseOf tc:isContributorOf .

# Specific Object Property for MediaContent
tc:hasContentContributor a owl:ObjectProperty ;
    rdfs:label "has content contributor" ;
    rdfs:comment "Links media content to contributors responsible for its creation." ;
    rdfs:subPropertyOf tc:hasContributor ;
    rdfs:domain tc:MediaContent ;
    rdfs:range tc:Contributors .

# Link Contributors to Roles (within the context of Creation)
tc:hasRole a owl:ObjectProperty ;
    rdfs:label "has role" ;
    rdfs:comment "Links a Contributor to a role they perform in the creation of media content." ;
    rdfs:domain tc:Contributors ;
    rdfs:range tc:Role .

# Property linking Role to Agent
tc:isPerformedBy a owl:ObjectProperty ;
    rdfs:label "is performed by" ;
    rdfs:comment "Links a role to the agent who performed it." ;
    rdfs:domain tc:Role ;
    rdfs:range tc:Agent .

# Inverse Property linking Agent to Role
tc:performsRole a owl:ObjectProperty ;
    rdfs:label "performs role" ;
    rdfs:comment "Indicates that an agent performs or fulfills a specific role." ;
    rdfs:domain tc:Agent ;
    rdfs:range tc:Role .

# Declare the inverse relationship
tc:isPerformedBy owl:inverseOf tc:performsRole .

tc:rolePeriod a owl:ObjectProperty ;
    rdfs:label "Role Period" ;
    rdfs:comment "Specifies the time period during which the agent performed the role." ;
    rdfs:domain tc:Role ;
    rdfs:range time:TemporalEntity .

# Links Consumption to MediaContent
tc:consumptionFor a owl:ObjectProperty ;
    rdfs:label "consumption for" ;
    rdfs:comment "Links the consumption data to the media content that was consumed." ;
    rdfs:domain tc:Consumption ;
    rdfs:range tc:MediaContent .

# Reciprocal Property linking MediaContent to its Consumption
tc:hasConsumption a owl:ObjectProperty ;
    rdfs:label "has consumption" ;
    rdfs:comment "Links media content to the consumption events associated with it." ;
    rdfs:domain tc:MediaContent ;
    rdfs:range tc:Consumption .

# Declare the inverse relationship => should be aplied to other inverse relations
tc:consumptionFor owl:inverseOf tc:hasConsumption .

# Links Consumption to Publication

# Object Property: isBasedOnPublication
tc:isBasedOnPublication a owl:ObjectProperty ;
    rdfs:label "is based on publication" ;
    rdfs:comment "Links the consumption of media content to the publication it is based on." ;
    rdfs:domain tc:Consumption ;
    rdfs:range tc:Publication .

# Inverse Property: isPublicationOfConsumption
tc:isPublicationOfConsumption a owl:ObjectProperty ;
    rdfs:label "is publication of consumption" ;
    rdfs:comment "Indicates that a publication is associated with specific consumption data." ;
    rdfs:domain tc:Publication ;
    rdfs:range tc:Consumption .

# Object Property: Link MediaInstance to TechnicalMetadata
tc:hasTechnicalMetadata a owl:ObjectProperty ;
    rdfs:label "has technical metadata" ;
    rdfs:comment "Links the MediaInstance to its specific technical metadata." ;
    rdfs:domain tc:MediaInstance ;
    rdfs:range tc:TechnicalMetadata .

# Object Property: Inverse of hasTechnicalMetadata
tc:isTechnicalMetadataFor a owl:ObjectProperty ;
    rdfs:label "is technical metadata for" ;
    rdfs:comment "Links technical metadata to the media content it describes." ;
    rdfs:domain tc:TechnicalMetadata ;
    rdfs:range tc:MediaInstance .

# Declare the inverse relationship between hasTechnicalMetadata and isTechnicalMetadataFor
tc:hasTechnicalMetadata owl:inverseOf tc:isTechnicalMetadataFor .

# Specific Relationships of hasTechnicalMetadata

# Linking Video with its Technical Metadata
tc:hasVideoTechnicalMetadata a owl:ObjectProperty ;
    rdfs:label "has video technical metadata" ;
    rdfs:comment "Links video content to its specific technical metadata." ;
    rdfs:domain tc:MediaInstance ;
    rdfs:range tc:VideoTechnicalMetadata .

tc:isVideoTechnicalMetadataFor a owl:ObjectProperty ;
    rdfs:label "is video technical metadata for" ;
    rdfs:comment "Links video technical metadata to the video content it describes." ;
    rdfs:domain tc:VideoTechnicalMetadata ;
    rdfs:range tc:MediaInstance .

tc:hasVideoTechnicalMetadata owl:inverseOf tc:isVideoTechnicalMetadataFor .

# Linking Audio with its Technical Metadata
tc:hasAudioTechnicalMetadata a owl:ObjectProperty ;
    rdfs:label "has audio technical metadata" ;
    rdfs:comment "Links audio content to its specific technical metadata." ;
    rdfs:domain tc:MediaInstance ;
    rdfs:range tc:AudioTechnicalMetadata .

tc:isAudioTechnicalMetadataFor a owl:ObjectProperty ;
    rdfs:label "is audio technical metadata for" ;
    rdfs:comment "Links audio technical metadata to the audio content it describes." ;
    rdfs:domain tc:AudioTechnicalMetadata ;
    rdfs:range tc:MediaInstance .

tc:hasAudioTechnicalMetadata owl:inverseOf tc:isAudioTechnicalMetadataFor .

# Linking Image with its Technical Metadata
tc:hasImageTechnicalMetadata a owl:ObjectProperty ;
    rdfs:label "has image technical metadata" ;
    rdfs:comment "Links image content to its specific technical metadata." ;
    rdfs:domain tc:MediaInstance ;
    rdfs:range tc:ImageTechnicalMetadata .

tc:isImageTechnicalMetadataFor a owl:ObjectProperty ;
    rdfs:label "is image technical metadata for" ;
    rdfs:comment "Links image technical metadata to the image content it describes." ;
    rdfs:domain tc:ImageTechnicalMetadata ;
    rdfs:range tc:MediaInstance .

tc:hasImageTechnicalMetadata owl:inverseOf tc:isImageTechnicalMetadataFor .

# Linking 3D Model with its Technical Metadata
tc:has3DModelTechnicalMetadata a owl:ObjectProperty ;
    rdfs:label "has 3D model technical metadata" ;
    rdfs:comment "Links 3D model content to its specific technical metadata." ;
    rdfs:domain tc:MediaInstance ;
    rdfs:range tc:3DModelTechnicalMetadata .

tc:is3DModelTechnicalMetadataFor a owl:ObjectProperty ;
    rdfs:label "is 3D model technical metadata for" ;
    rdfs:comment "Links 3D model technical metadata to the 3D model content it describes." ;
    rdfs:domain tc:3DModelTechnicalMetadata ;
    rdfs:range tc:MediaInstance .

tc:has3DModelTechnicalMetadata owl:inverseOf tc:is3DModelTechnicalMetadataFor .


# Object Property: hasLocation linking any class to Location
tc:hasLocation a owl:ObjectProperty ;
    rdfs:label "has location" ;
